ogr2ogr -f "PostgreSQL" PG:"host=localhost user=postgres dbname=suomen_akatemia password=salasana t채h채n" 2001-2023.csv -nln suomen_akatemia_kaikki_2001-2023 -oo AUTODETECT_TYPE=YES

delete from suomen_akatemia_bioalat_2001_2023
where ogc_fid = 1;

