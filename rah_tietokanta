- Table: public.rahoitukset_2001_2023

-- DROP TABLE IF EXISTS public.rahoitukset_2001_2023;

CREATE TABLE IF NOT EXISTS public.rahoitukset_2001_2023
(
    haku character varying(100) COLLATE pg_catalog."default",
    organisaatio character varying(100) COLLATE pg_catalog."default",
    tutkimusaihe character varying(200) COLLATE pg_catalog."default",
    hakija character varying(100) COLLATE pg_catalog."default",
    "päätös" integer,
    pvm date,
    rahoituskausi character varying(100) COLLATE pg_catalog."default",
    rahoitus integer
)

TABLESPACE pg_default;

ALTER TABLE IF EXISTS public.rahoitukset_2001_2023
    OWNER to nimipoistettu;
